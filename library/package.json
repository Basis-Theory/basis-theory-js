{
  "name": "@basis-theory/basis-theory-js",
  "version": "1.25.1",
  "repository": "https://github.com/Basis-Theory/basis-theory-js",
  "license": "Apache-2.0",
  "author": {
    "name": "Basis Theory",
    "email": "support@basistheory.com"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "yarn clean && concurrently \"yarn build:bundle\" \"yarn build:module\" \"yarn build:declarations\" \"yarn build:package\" --kill-others-on-fail",
    "build:bundle": "webpack --config webpack.prod.js",
    "build:declarations": "tsc",
    "build:module": "babel src --extensions .ts --out-dir dist",
    "build:package": "node prepare.js",
    "clean": "rimraf dist",
    "prepare": "yarn build",
    "release": "semantic-release",
    "size": "size-limit",
    "start": "webpack --config webpack.dev.js --watch",
    "test": "jest",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "@basis-theory/basis-theory-elements-interfaces": "^4.0.0",
    "axios": "^0.21.2",
    "camelcase-keys": "^6.2.2",
    "os": "^0.1.2",
    "os-browserify": "^0.3.0",
    "snake-case": "^3.0.4",
    "snakecase-keys": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.16",
    "@babel/core": "^7.15.8",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-transform-arrow-functions": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-typescript": "^7.12.13",
    "@peculiar/webcrypto": "^1.1.6",
    "@size-limit/preset-small-lib": "^7.0.5",
    "@types/chance": "^1.1.3",
    "@types/dotenv-webpack": "^7.0.1",
    "@types/eccrypto": "^1.1.3",
    "@types/is-base64": "^1.1.0",
    "@types/jest": "^27.4.0",
    "axios-mock-adapter": "^1.19.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-inline-dotenv": "^1.6.0",
    "chance": "^1.1.8",
    "concurrently": "5.3.0",
    "dotenv-webpack": "^7.0.2",
    "is-base64": "^1.1.0",
    "jest": "^27.4.5",
    "jest-environment-jsdom": "^27.4.4",
    "rimraf": "^3.0.2",
    "size-limit": "^7.0.5",
    "ts-jest": "^27.1.2",
    "typescript": "4.1.4",
    "uuid": "^8.3.2",
    "webpack": "^5.20.2",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.5.0",
    "webpack-merge": "^5.8.0"
  },
  "engines": {
    "node": ">=10.12.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "size-limit": [
    {
      "path": "dist/basis-theory-js.bundle.js",
      "limit": "50 KB"
    }
  ]
}
