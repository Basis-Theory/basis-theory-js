{
  "name": "@Basis-Theory/basis-theory-js",
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "yarn build --watch",
    "build": "webpack --mode=development",
    "test": "jest",
    "test:cov": "jest --coverage",
    "lint": "tsdx lint",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "start:server": "ts-node --project ./server/tsconfig.json -r tsconfig-paths/register ./server/index.ts"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "James Armstead",
  "module": "dist/basis-theory-js.esm.js",
  "size-limit": [
    {
      "path": "dist/basis-theory-js.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/basis-theory-js.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-transform-arrow-functions": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-typescript": "^7.12.13",
    "@size-limit/preset-small-lib": "^4.9.1",
    "@types/eccrypto": "^1.1.2",
    "@types/is-base64": "^1.1.0",
    "@types/jest": "^26.0.20",
    "babel-loader": "^8.2.2",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.8",
    "is-base64": "^1.1.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "size-limit": "^4.9.1",
    "ts-jest": "^26.5.0",
    "tsdx": "^0.14.1",
    "typescript": "4.1.4",
    "webpack": "^5.20.2",
    "webpack-cli": "^4.5.0"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "buffer": "^6.0.3",
    "core-js": "^3.8.3",
    "eccrypto-js": "^5.4.0",
    "process": "^0.11.10"
  }
}
