{
  "name": "@Basis-Theory/basis-theory-js",
  "version": "1.0.0-alpha.1",
  "repository": "https://github.com/Basis-Theory/basis-theory-js",
  "license": "UNLICENSED",
  "author": "James Armstead",
  "main": "dist/basis-theory.bundle.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "concurrently \"yarn build:bundle\" \"yarn build:module\" \"yarn build:declarations\" --kill-others-on-fail",
    "build:bundle": "webpack --config webpack.prod.js",
    "build:declarations": "tsc",
    "build:module": "babel src --extensions .ts --out-dir dist",
    "prepare": "yarn build",
    "release": "semantic-release",
    "size": "size-limit",
    "start": "webpack --config webpack.dev.js --watch",
    "test": "jest",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "core-js": "^3.8.3",
    "process": "^0.11.10"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.16",
    "@babel/core": "^7.12.13",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-transform-arrow-functions": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-typescript": "^7.12.13",
    "@peculiar/webcrypto": "^1.1.6",
    "@size-limit/preset-small-lib": "^4.9.1",
    "@types/eccrypto": "^1.1.2",
    "@types/is-base64": "^1.1.0",
    "@types/jest": "^26.0.20",
    "babel-loader": "^8.2.2",
    "concurrently": "5.3.0",
    "is-base64": "^1.1.0",
    "jest": "^26.6.3",
    "jest-environment-jsdom": "^26.6.2",
    "size-limit": "^4.9.1",
    "ts-jest": "^26.5.0",
    "typescript": "4.1.4",
    "webpack": "^5.20.2",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.5.0",
    "webpack-merge": "^5.7.3"
  },
  "engines": {
    "node": ">=10"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "size-limit": [
    {
      "path": "dist/basis-theory.bundle.js",
      "limit": "50 KB"
    }
  ]
}
