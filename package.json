{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "start:server": "ts-node --project ./server/tsconfig.json -r tsconfig-paths/register ./server/index.ts"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "basis-theory-js",
  "author": "James Armstead",
  "module": "dist/basis-theory-js.esm.js",
  "size-limit": [
    {
      "path": "dist/basis-theory-js.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/basis-theory-js.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.9.1",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.11",
    "@types/uuid": "^8.3.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "eslint-plugin-prettier": "^3.3.1",
    "express": "^4.17.1",
    "husky": "^4.3.8",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "size-limit": "^4.9.1",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3",
    "uuid": "^8.3.2"
  },
  "dependencies": {
    "axios": "^0.21.1"
  },
  "jest": {
    "automock": false,
    "setupFiles": [
      "./setupJest.ts"
    ]
  }
}
