{
  "name": "@basis-theory/basis-theory-js",
  "version": "2.7.0",
  "repository": "https://github.com/Basis-Theory/basis-theory-js",
  "license": "Apache-2.0",
  "author": {
    "name": "Basis Theory",
    "email": "support@basistheory.com"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "prettier": "@basis-theory/eslint-config/prettier",
  "scripts": {
    "analyze": "size-limit --why",
    "build": "yarn clean && concurrently 'yarn build:bundle' 'yarn build:module' 'yarn build:declarations' 'yarn build:package' -n 'bundle,module,declarations,package' -c 'yellow,magenta,blue,green' --kill-others-on-fail -g",
    "build:bundle": "webpack --config webpack.prod.js",
    "build:declarations": "tsc && tsc-alias",
    "build:module": "babel src --extensions .ts --out-dir dist",
    "build:package": "node prepare.js",
    "clean": "rimraf dist",
    "cypress:ci": "cypress run --headless -b chrome",
    "cypress:open": "cypress open",
    "lint": "eslint . --quiet --ignore-path .gitignore",
    "lint:fix": "eslint . --fix --ignore-path .gitignore",
    "prepare": "yarn build",
    "release": "semantic-release",
    "size": "size-limit",
    "start": "webpack --config webpack.dev.js --watch",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:e2e": "yarn cypress:ci",
    "pretty-quick": "pretty-quick --staged",
    "postinstall": "husky install"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "camelcase-keys": "^6.2.2",
    "csstype": "^3.0.11",
    "os": "^0.1.2",
    "os-browserify": "^0.3.0",
    "snake-case": "^3.0.4",
    "snakecase-keys": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.15",
    "@babel/core": "^7.23.5",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-transform-arrow-functions": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.22.15",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-typescript": "^7.12.13",
    "@basis-theory/eslint-config": "^1.0.13",
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^17.6.3",
    "@peculiar/webcrypto": "^1.4.3",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^8.0.5",
    "@semantic-release/npm": "^7.0.10",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@size-limit/preset-small-lib": "^7.0.5",
    "@swc/core": "^1.3.75",
    "@swc/jest": "^0.2.28",
    "@types/chance": "^1.1.3",
    "@types/dotenv-webpack": "^7.0.4",
    "@types/eccrypto": "^1.1.5",
    "@types/is-base64": "^1.1.1",
    "@types/jest": "^27.4.0",
    "@types/uuid": "^8.3.3",
    "axios-mock-adapter": "^1.21.2",
    "babel-loader": "^8.2.2",
    "babel-plugin-inline-dotenv": "^1.1.0",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-plugin-tsconfig-paths-module-resolver": "^1.0.3",
    "chance": "^1.1.11",
    "concurrently": "^8.2.2",
    "cypress": "^12.16.0",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^7.32.0",
    "husky": "^8.0.1",
    "is-base64": "^1.1.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.3",
    "rimraf": "^3.0.2",
    "semantic-release": "^19.0.3",
    "size-limit": "^7.0.5",
    "ts-jest": "^29.1.1",
    "tsc-alias": "^1.7.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^5.0.4",
    "uuid": "^8.3.2",
    "webpack": "^5.84.1",
    "webpack-bundle-analyzer": "^4.9.0",
    "webpack-cli": "^4.10.0",
    "webpack-merge": "^5.8.0"
  },
  "resolutions": {
    "glob-parent": "^5.1.2",
    "postcss": "^8.4.31",
    "normalize-url": "^5.3.1",
    "is-svg": "^4.2.2",
    "yargs-parser": "^21.0.0",
    "mem": "^4.0.0",
    "semver-regex": "^3.1.4",
    "tmpl": "^1.0.5",
    "tar": "^4.4.19",
    "minimist": "^1.2.6",
    "json-schema": "^0.4.0",
    "terser": "^5.14.2",
    "word-wrap": "^1.2.5"
  },
  "engines": {
    "node": ">=10.12.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "size-limit": [
    {
      "path": "dist/basis-theory-js.bundle.js",
      "limit": "50 KB"
    }
  ]
}
